IF OBJECT_ID ('dbo.v_EquipmentProperty', 'V') IS NOT NULL
	DROP VIEW dbo.v_EquipmentProperty;
GO

CREATE VIEW dbo.v_EquipmentProperty
AS
SELECT ep.ID,
       ep.Description,
       ep.Value,
       ep.EquipmentProperty,
       ep.EquipmentCapabilityTestSpecification,
       ep.TestResult,
       ep.EquipmentID,
       ep.ClassPropertyID
FROM dbo.EquipmentProperty ep
GO
